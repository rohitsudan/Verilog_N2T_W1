#! /usr/local/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x229ddf0 .scope module, "testbench_simple" "testbench_simple" 2 2;
 .timescale -9 -11;
P_0x229df70 .param/l "width" 0 2 3, +C4<00000000000000000000000000001000>;
v0x22f9270_0 .var "in", 7 0;
v0x22f9360_0 .net "out", 0 0, v0x22f9140_0;  1 drivers
S_0x22d4350 .scope module, "myOR" "OR_8way" 2 6, 3 1 0, S_0x229ddf0;
 .timescale -9 -11;
    .port_info 0 /INPUT 8 "in"
    .port_info 1 /OUTPUT 1 "out"
P_0x22d4520 .param/l "width" 0 3 2, +C4<00000000000000000000000000001000>;
v0x22d4680_0 .var/i "i", 31 0;
v0x22f9060_0 .net "in", 7 0, v0x22f9270_0;  1 drivers
v0x22f9140_0 .var "out", 0 0;
E_0x22d51f0 .event edge, v0x22f9060_0;
    .scope S_0x22d4350;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x22d4680_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_0x22d4350;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x22f9140_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x22d4350;
T_2 ;
    %wait E_0x22d51f0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x22f9140_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x22d4680_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x22d4680_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x22f9140_0;
    %load/vec4 v0x22f9060_0;
    %load/vec4 v0x22d4680_0;
    %part/s 1;
    %or;
    %store/vec4 v0x22f9140_0, 0, 1;
    %delay 100, 0;
    %load/vec4 v0x22d4680_0;
    %addi 1, 0, 32;
    %store/vec4 v0x22d4680_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x229ddf0;
T_3 ;
    %vpi_call 2 9 "$dumpfile", "testbench_simple.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x229ddf0 {0 0 0};
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x22f9270_0, 0, 8;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x22f9270_0, 0, 8;
    %delay 1000, 0;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x22f9270_0, 0, 8;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x22f9270_0, 0, 8;
    %delay 1000, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x22f9270_0, 0, 8;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench_simple.v";
    "simple.v";
