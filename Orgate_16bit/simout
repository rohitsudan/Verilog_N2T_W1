#! /usr/local/bin/vvp
:ivl_version "10.0 (stable)" "(v10_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x108edc0 .scope module, "testbench_simple" "testbench_simple" 2 2;
 .timescale -9 -11;
P_0x108ef40 .param/l "width" 0 2 3, +C4<00000000000000000000000000010000>;
v0x10e9e20_0 .var "in0", 15 0;
v0x10e9f10_0 .var "in1", 15 0;
v0x10e9fe0_0 .net "out", 15 0, v0x10e9cb0_0;  1 drivers
S_0x10c5ae0 .scope module, "mymux" "or16_bit" 2 7, 3 2 0, S_0x108edc0;
 .timescale -9 -11;
    .port_info 0 /INPUT 16 "in0"
    .port_info 1 /INPUT 16 "in1"
    .port_info 2 /OUTPUT 16 "out"
P_0x10c5cb0 .param/l "width" 0 3 3, +C4<00000000000000000000000000010000>;
v0x108efe0_0 .net "in0", 15 0, v0x10e9e20_0;  1 drivers
v0x10e9bd0_0 .net "in1", 15 0, v0x10e9f10_0;  1 drivers
v0x10e9cb0_0 .var "out", 15 0;
E_0x108f7a0 .event edge, v0x10e9bd0_0, v0x108efe0_0;
    .scope S_0x10c5ae0;
T_0 ;
    %wait E_0x108f7a0;
    %load/vec4 v0x108efe0_0;
    %load/vec4 v0x10e9bd0_0;
    %or;
    %store/vec4 v0x10e9cb0_0, 0, 16;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x108edc0;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "testbench_simple.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x108edc0 {0 0 0};
    %pushi/vec4 755, 0, 16;
    %store/vec4 v0x10e9e20_0, 0, 16;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x10e9f10_0, 0, 16;
    %delay 100, 0;
    %pushi/vec4 755, 0, 16;
    %store/vec4 v0x10e9e20_0, 0, 16;
    %pushi/vec4 65535, 0, 16;
    %store/vec4 v0x10e9f10_0, 0, 16;
    %delay 100, 0;
    %pushi/vec4 755, 0, 16;
    %store/vec4 v0x10e9e20_0, 0, 16;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x10e9f10_0, 0, 16;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench_simple.v";
    "simple.v";
